{
  "name": "@knapsack/renderer-twig",
  "version": "1.6.0",
  "description": "Render Twig",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn build:js",
    "build:js": "BABEL_ENV=cli babel --config-file @knapsack/babel-config --extensions .js,.ts --out-dir dist/ src/",
    "build:types": "tsc",
    "clean": "rm -rf dist ./node_modules/.cache/tsconfig.tsbuildinfo",
    "test": "yarn build:types",
    "watch": "yarn build:js --watch"
  },
  "dependencies": {
    "@basalt/twig-renderer": "^0.12.2"
  },
  "peerDependencies": {
    "@basalt/knapsack": "^1.0.0"
  },
  "author": "Basalt (https://basalt.io/)",
  "publishConfig": {
    "access": "public"
  }
}
