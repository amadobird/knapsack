{
  "name": "@knapsack/utils",
  "license": "GPL-2.0-or-later",
  "version": "1.8.0",
  "description": "A Knapsack component that helps build Design Systems",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build": "babel --config-file @knapsack/babel-config/es --source-maps --ignore 'src/*.test.js' --extensions .js,.ts --out-dir dist/ src/",
    "prepublishOnly": "yarn build",
    "start": "yarn build --watch"
  },
  "dependencies": {
    "@knapsack/core": "^1.8.0",
    "color": "^3.0.0"
  },
  "devDependencies": {
    "@knapsack/babel-config": "^1.8.0",
    "prop-types": "^15.7.2"
  },
  "author": "Basalt (https://basalt.io/)",
  "publishConfig": {
    "access": "public"
  }
}
