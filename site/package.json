{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@basalt/demo-data": "0.1.1",
    "@basalt/twig-renderer": "0.7.3",
    "gatsby": "next",
    "gatsby-cli": "next",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-catch-links": "^1.0.24",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-source-filesystem": "^1.5.39",
    "gatsby-transformer-remark": "^1.7.44",
    "react-helmet": "^5.2.0",
    "react-jsonschema-form": "1.0.3",
    "react-table": "6.8.6"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build:gatsby": "gatsby build",
    "prep": "node twig.js",
    "postinstall": "composer install",
    "serve:twig": "php -S 127.0.0.1:3001 node_modules/@basalt/twig-renderer/dist/server--sync.php",
    "build": "npm run prep && npm-run-all --parallel build:gatsby",
    "start": "npm run prep && npm-run-all --parallel serve:twig watch:gatsby",
    "lint": "eslint --ext .js,.jsx .",
    "lint:fix": "eslint --ext .js,.jsx . --fix",
    "test": "npm run lint",
    "watch:gatsby": "sleep 1 && gatsby develop"
  },
  "devDependencies": {
    "npm-run-all": "4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
