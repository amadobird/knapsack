{
  "json.schemas": [
    {
      "fileMatch": ["**/knapsack.pattern.*.json"],
      "url": "./knapsack/src/json-schemas/schemaKnapsackPattern.json"
    },
    {
      "fileMatch": ["**/knapsack.navs.json"],
      "url": "./knapsack/src/json-schemas/schemaKnapsackNavsConfig.json"
    },
    {
      "fileMatch": ["**/knapsack.asset-sets.json"],
      "url": "./knapsack/src/json-schemas/schemaKnapsackAssetSetsConfig.json"
    },
    {
      "fileMatch": ["**/knapsack.settings.json"],
      "url": "./knapsack/src/json-schemas/schemaKnapsackSettings.json"
    },
    {
      "fileMatch": ["cypress.json"],
      "url": "https://raw.githubusercontent.com/cypress-io/cypress/develop/cli/schema/cypress.schema.json"
    }
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "html.customData": ["./examples/bootstrap/dist/meta/knapsack.html-data.json"],
  "html.suggest.html5": false
}
